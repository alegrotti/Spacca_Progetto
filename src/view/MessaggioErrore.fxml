<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<Pane fx:id="sfondo" prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MessaggioErroreController">
   <children>
      <VBox alignment="CENTER" prefHeight="300.0" prefWidth="300.0">
         <children>
            <Label fx:id="testoMessaggioErrore" alignment="CENTER" prefHeight="150.0" prefWidth="250.0" />
            <Button fx:id="bottoneOk" mnemonicParsing="false" onAction="#mouseOkClicked" prefHeight="37.0" prefWidth="136.0" text="OK">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</Pane>
