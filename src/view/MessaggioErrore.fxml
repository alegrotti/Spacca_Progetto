<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<Pane fx:id="sfondo" prefHeight="158.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MessaggioErroreController">
   <children>
      <VBox fx:id="sfondoBianco" alignment="CENTER" prefHeight="158.0" prefWidth="300.0">
         <children>
            <HBox alignment="CENTER">
               <children>
                  <ImageView fx:id="immagine" fitHeight="55.0" fitWidth="54.0" pickOnBounds="true" preserveRatio="true">
                     <HBox.margin>
                        <Insets left="10.0" right="10.0" />
                     </HBox.margin>
                  </ImageView>
                  <Label fx:id="testoMessaggioErrore" alignment="CENTER" prefHeight="150.0" prefWidth="250.0" />
               </children>
            </HBox>
            <Button fx:id="bottoneOk" mnemonicParsing="false" onAction="#mouseOkClicked" prefHeight="37.0" prefWidth="136.0" text="OK">
               <VBox.margin>
                  <Insets bottom="10.0" top="10.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</Pane>
